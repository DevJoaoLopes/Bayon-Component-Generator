{
  "name": "vscode-bayon-component-generator",
  "publisher": "devjoaolopes",
  "displayName": "Bayon Component Generator",
  "description": "Extension for generate components using Bayon Design System in React",
  "icon": "art/icon.png",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/DevJoaoLopes/Bayon-Component-Generator"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "Bayon",
    "React",
    "Component Generator",
    "Jest"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bayon-component-generator.generate",
        "title": "Bayon Generate Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "bayon-component-generator.generate",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "jest --silent",
    "test:dev": "jest --silent=false",
    "coverage": "jest --coverage --silent"
  },
  "devDependencies": {
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "@babel/core": "^7.23.9",
    "@babel/preset-typescript": "^7.24.1",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.30",
    "@types/vscode": "1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.8",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3"
  }
}
